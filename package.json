{
  "name": "food-app",
  "version": "1.0.0",
  "description": "Food donation and distribution platform",
  "private": true,
  "workspaces": [
    "app/*"
  ],
  "scripts": {
    "dev": "cd app/login-and-registration/login-and-registration-ui && npx vite",
    "build": "cd app/login-and-registration/login-and-registration-ui && npx tsc && npx vite build",
    "serve": "cd app/login-and-registration/login-and-registration-ui && npx vite preview",
    "start:backend": "cd app/login-and-registration/login-and-registration-backend && mvn spring-boot:run",
    "start": "npx concurrently \"npm:start:backend\" \"npm:dev\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "axios": "^1.4.0",
    "styled-components": "^6.0.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "typescript": "^5.1.6",
    "concurrently": "^8.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
